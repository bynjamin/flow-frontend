    "scripts": {
        "dev": "yarn run tailwind && react-scripts start",
        "build": "yarn run tailwind && yarn run purge-tailwind && cross-env GENERATE_SOURCEMAP=false react-scripts build",
        "clean": "rimraf node_modules",
        "lint:ts": "eslint --ext=ts,tsx src",
        "lint:js": "eslint --ext=js,jsx src",
        "graphql:schema": "better-npm-run graphql:schema && better-npm-run public-graphql:schema",
        "graphql:types": "yarn main-graphql:types && yarn public-graphql:types",
        "main-graphql:types": "apollo codegen:generate --localSchemaFile=schema.json --target=typescript  --tagName=gql --addTypename --globalTypesFile=src/app/__generated__/globalTypes.ts",
        "public-graphql:types": "apollo codegen:generate --localSchemaFile=publicSchema.json --target=typescript  --tagName=publicGql --addTypename --globalTypesFile=src/public-app/__generated__/globalTypes.ts",
        "tailwind": "tailwind build ./src/styles/tailwind-config.css -c ./tailwind.config.js -o ./src/styles/tailwind.css",
        "purge-tailwind": "node ./purge-tailwindcss.js",
        "test": "react-scripts test --env=node --watchAll=false --passWithNoTests",
        "test:watch": "react-scripts test --env=node",
        "eject": "react-scripts eject",
        "build-docs": "find ./src/app/main/documentation/material-ui-components \\( -name '*.ts' -o -name '*.tsx' \\) -type f -delete && babel-node src/app/main/documentation/material-ui-components/build.js",
        "pnpify-sdk": "yarn pnpify --sdk",
        "bundle-report": "yarn run tailwind && node ./purge-tailwindcss.js && cross-env GENERATE_SOURCEMAP=false craco build"
    },
    "betterScripts": {
        "graphql:schema": "cross-env apollo service:download --endpoint $REACT_APP_API_URL",
        "public-graphql:schema": "cross-env apollo service:download publicSchema.json --endpoint $REACT_APP_PUBLIC_API_URL"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "ie 11"
        ]
    }
